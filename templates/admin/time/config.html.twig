{% extends 'admin/bars.html.twig' %}

{% block contenpage %}
    <div class="">
        <div class="page-title">
            <div class="title_left">
                <h5><a href="{{ path('app_admin_blogPosts') }}">Config</a> >> Blog</h5>
            </div>

        </div>

        <div class="clearfix"></div>
        <div class="row">
            <h6>Imported files</h6>
            <ul>
                {%  for  importFile  in importFiles %}
                <li>
                    {{ importFile }}
                </li>
                {%  endfor %}
            </ul>
        </div>
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">

                    <div class="x_content">
                        {{ form_start(form) }}
                        {{ form_widget(form) }}
                        <button class="btn btn-primary m-btn m-btn--icon" type="submit">
                            <i class="la la-save"></i>
                            LÆ°u
                        </button>
                        {{ form_end(form) }}

                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
